#include <stdio.h>
#pragma warning(disable:4996)
int main()
{
	int i;
	int g;
	int sum;
	int num;
	double avg;
	i = 0;
	sum = 0;

	printf("학생 수");
	scanf("%d",&num);
	
	while (i<num)
	{
		printf("점수 입력");
		scanf("%d", &g);
		sum += g;
		i++;
	
	}
	avg = sum / (double)num;
	printf("%f입니다", avg);

	return 0;
}
